{
  "name": "nestjs-backend-base",
  "version": "1.2.0",
  "description": "Backend base con Nest.js, TypeORM y TypeScript",
  "author": "agetic@agetic.gob.bo",
  "private": true,
  "license": "LPG-Bolivia",
  "packageManager": "npm@10.8.2",
  "scripts": {
    "build": "nest build",
    "build-docker": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format-check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "npm run migrations:clean && npm run schema:drop && npm run migrations:generate database/migrations/init && npm run migrations:run && npm run seeds:run",
    "dev": "LOG_SQL=true LOG_CONSOLE=true LOG_LEVEL=debug npm run start:dev",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "db:create": "bash ./database/scripts/dbcreate_docker.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --coverage=false",
    "test:single": "NODE_ENV=test jest --coverage=false -i",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "NODE_OPTIONS=--max_old_space_size=8192 ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:clean": "rimraf database/migrations/*",
    "migrations:generate": "npm run typeorm -- migration:generate -d ormconfig-default.ts",
    "migrations:run": "npm run typeorm -- migration:run -d ormconfig-default.ts",
    "migrations:revert": "npm run typeorm -- migration:revert -d ormconfig-default.ts",
    "seeds:create": "npm run typeorm -- migration:create",
    "seeds:run": "npm run typeorm -- migration:run -d ormconfig-seed.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ormconfig-default.ts",
    "rimraf": "./node_modules/rimraf/bin.js",
    "compodoc": "compodoc -p tsconfig.json -o --language es-ES --disableCoverage --hideGenerator",
    "db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig-default.ts -d ormconfig-default.ts -d docs/ERD.png",
    "prepare": "husky install",
    "check-node-version": "node ./node_modules/.bin/check-node-version",
    "prebuild": "npm run check-node-version && rimraf dist && npm run lint",
    "presetup": "npm run check-node-version",
    "prestart": "npm run check-node-version",
    "prestart:dev": "npm run check-node-version",
    "pretest": "npm run check-node-version",
    "preinstall": "mkdir -p node_modules && touch ./node_modules/.metadata_never_index",
    "postinstall": "npm run check-node-version",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@elartedelcodigo/check-node-version": "1.1.7",
    "@golevelup/ts-jest": "0.5.0",
    "@nestjs/axios": "3.0.3",
    "@nestjs/common": "10.4.5",
    "@nestjs/config": "3.2.3",
    "@nestjs/core": "10.4.5",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.5",
    "@nestjs/schedule": "4.1.1",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/typeorm": "10.0.2",
    "@types/bcrypt": "5.0.2",
    "@types/express-list-endpoints": "6.0.3",
    "@types/html-to-pdfmake": "2.4.4",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cls-rtracer": "2.6.3",
    "connect-typeorm": "2.0.0",
    "cookie-parser": "1.4.7",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "ejs": "3.1.10",
    "express-list-endpoints": "7.1.0",
    "express-session": "1.18.1",
    "file-stream-rotator": "1.0.0",
    "flatted": "3.3.1",
    "helmet": "7.2.0",
    "html-to-pdfmake": "2.5.13",
    "husky": "9.1.6",
    "ip-address": "9.0.5",
    "jsdom": "24.1.3",
    "nanoid": "3.3.7",
    "nest-authz": "2.11.0",
    "openid-client": "5.7.0",
    "pako": "2.1.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pdfmake": "0.2.10",
    "pg": "8.13.0",
    "pino": "8.21.0",
    "pino-loki": "2.3.1",
    "rimraf": "5.0.10",
    "rxjs": "7.8.1",
    "sql-formatter": "15.4.4",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.20",
    "typeorm-adapter": "1.7.0",
    "uuid": "10.0.0",
    "zxcvbn-typescript": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@compodoc/compodoc": "1.1.26",
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.2.2",
    "@nestjs/testing": "10.4.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.21",
    "@types/express-session": "1.18.0",
    "@types/ip": "1.1.3",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.12",
    "@types/pako": "2.0.3",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/pdfmake": "0.2.9",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@types/zxcvbn": "4.4.5",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "commit-and-tag-version": "12.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "3.3.3",
    "supertest": "6.3.4",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ],
    "coveragePathIgnorePatterns": [
      "src/main.ts",
      "src/database/migrations/",
      "src/database/seeds/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts",
      ".guard.ts",
      ".enum.ts"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "engines": {
    "node": "^20",
    "npm": ">=10"
  }
}
